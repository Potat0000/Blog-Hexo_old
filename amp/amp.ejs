    <style amp-custom="">
        html {
            font-size:62.5%;
            color:#2c2c2c;
            background:#fcfcfc
        }
        html:lang(en) {
            font-family:Helvetica Neue,Helvetica,Arial,sans-serif
        }
        html:lang(zh-cmn-Hans) {
            font-family:Helvetica Neue,Helvetica,Arial,微软雅黑,黑体,Microsoft YaHei,メイリオ,Meiryo,sans-serif
        }
        a {
            text-decoration:none;
            color:#47b7cc
        }
        a:hover {
            text-decoration:underline
        }
        .clearfix:after,.pagination:after {
            display:block;
            clear:both;
            overflow:hidden;
            height:0;
            content:''
        }
        .header {
            max-width:640px;
            margin:auto;
            padding:5rem 1rem 0;
            border-top:5px solid #47b7cc
        }
        .header__logo {
            text-align:center
        }
        .header__logo a:hover {
            text-decoration:none
        }
        .header__logo__mainHeadline {
            font-size:2.5rem;
            font-weight:400;
            display:inline-block;
            margin:0 0 3.5rem
        }
        .header__logo__subHeadline {
            font-weight:400;
            margin:15px 0
        }
        .header__menu {
            overflow:hidden
        }
        .header__menu__list {
            overflow:hidden;
            margin:0;
            padding:0;
            list-style:none;
            text-align:center
        }
        .header__menu__list__item {
            display:inline-block;
            margin:0 1rem
        }
        .header__menu__list__item a {
            font-size:1.4rem
        }
        @media screen and (min-width:35.5em) {
            .header {
                display:-webkit-flex;
                display:-ms-flexbox;
                display:flex;
                padding-right:0;
                padding-left:0;
                -webkit-align-items:center;
                -ms-flex-align:center;
                align-items:center
            }
            .header__logo {
                margin: 0 auto;
                text-align:left
            }
            .header__logo__mainHeadline {
                margin:0
            }
            .header__menu {
                float:right
            }
        }
        #content {
            max-width:640px;
            margin:auto;
            padding:0 1rem
        }
        .article {
            font-size:1.6rem
        }
        .article__header__meta {
            overflow:hidden;
            height:20px;
            text-align:center;
            float: right
        }
        .article__header__meta__time {
            display:block;
            float:left;
            height:100%
        }
        .article__header__meta__time a {
            font-size:1.3rem;
            font-weight:700;
            display:block;
            text-decoration:none;
            color:#aaa
        }
        .article__header__title {
            font-size:2.5rem;
            text-align:center;
            font-weight:400
        }
        .article__header__title a {
            color:inherit
        }
        .article__header__title a:hover {
            text-decoration:none
        }
        .article.link .article__header__title {
            margin:1rem 0
        }
        .article__entry {
            line-height:2;
            margin-bottom:15px;
            padding-top:1rem
        }
        .article__entry .video-container,.article__entry blockquote,.article__entry dl,.article__entry h3,.article__entry h4,.article__entry h5,.article__entry h6,.article__entry iframe,.article__entry ol,.article__entry p,.article__entry table,.article__entry ul {
            margin-top:15px
        }
        .article__entry blockquote {
            font-family:Georgia,Times New Roman,serif;
            margin:3rem .5rem;
            padding:0 20px;
            border-left:1px solid #47b7cc
        }
        .article__entry blockquote footer {
            font-size:.8em;
            line-height:1;
            margin:20px 0 0;
            padding-top:0;
            color:#4e5359;
            border-top:none
        }
        .article__entry blockquote footer cite:before {
            padding:0 .5em;
            content:'—';
            color:#ccc
        }
        .article__entry code,.article__entry pre {
            font-family:Monaco,Menlo,Consolas,Courier New,monospace
        }
        .article__entry code {
            font-size:.9em;
            margin:0 2px;
            padding:0 5px;
            color:#666;
            border:1px solid #7c848c;
            border-radius:3px;
            background:#f9f9f9
        }
        .article__entry pre {
            overflow:auto;
            padding:7px 15px;
            border:1px solid #7c848c
        }
        .article__entry pre code {
            margin:0;
            padding:0;
            border:none;
            border-radius:0;
            background:none
        }
        .article__entry dl,.article__entry ol,.article__entry ul {
            margin-left:20px;
            padding:0
        }
        .article__entry dl dl,.article__entry dl ol,.article__entry dl ul,.article__entry ol dl,.article__entry ol ol,.article__entry ol ul,.article__entry ul dl,.article__entry ul ol,.article__entry ul ul {
            margin-top:0
        }
        .article__entry h1,.article__entry h2 {
            font-weight:400;
            line-height:1;
            margin-top:1.5em
        }
        .article__entry h3,.article__entry h4,.article__entry h5,.article__entry h6 {
            font-weight:400;
            line-height:1
        }
        .article__entry img,.article__entry video {
            max-width:100%;
            height:auto;
            border:none
        }
        .article__entry iframe {
            border:none
        }
        .article__entry .caption {
            font-size:.9em;
            position:relative;
            display:block;
            margin-top:5px;
            padding-left:25px;
            color:#362f3c
        }
        .article__entry .video-container {
            position:relative;
            overflow:hidden;
            height:0;
            padding-top:30px;
            padding-bottom:56.25%
        }
        .article__entry .video-container embed,.article__entry .video-container iframe,.article__entry .video-container object {
            position:absolute;
            top:0;
            left:0;
            width:100%;
            height:100%;
            margin-top:0
        }
        .article__entry .pullquote {
            font-size:1.5em;
            float:right;
            width:45%;
            margin:1em 0 .5em 1.5em;
            padding:0;
            text-align:left;
            border:none
        }
        .article__entry table {
            border-collapse:collapse
        }
        .article__entry table td {
            padding:2px 10px;
            border:1px solid #7c848c
        }
        .article__footer {
            padding:0 0 5px;
            border-bottom:1px solid #47b7cc
        }
        .pagination {
            font-size:1.4rem;
            margin:3rem 0
        }
        .pagination a {
            font-weight:700;
            display:inline-block;
            padding:1rem 1.1rem;
            border:1px solid #47b7cc;
            border-radius:4px
        }
        .pagination__prev {
            float:left
        }
        .pagination__prev:before {
            display:inline-block;
            margin-right:.5rem;
            content:'«';
            text-align:center
        }
        .pagination__next {
            float:right
        }
        .pagination__next:after {
            display:inline-block;
            margin-left:.5rem;
            content:'»';
            text-align:center
        }
        .archive {
            border-bottom:1px solid #47b7cc
        }
        .archive-title {
            font-size:2.5rem;
            font-weight:400;
            color:#362f3c
        }
        .search input {
            font-size:1.6rem;
            box-sizing:border-box;
            width:100%;
            padding:1rem;
            color:#362f3c;
            border:1px solid #7c848c
        }
        .search input:focus {
            border-color:#47b7cc
        }
        .widget {
            margin-bottom:30px
        }
        .widget ul.entry {
            font-size:14px;
            line-height:2;
            overflow:hidden;
            margin:0;
            padding:0;
            list-style:none
        }
        .widget ul.entry li {
            float:left;
            margin-right:30px
        }
        .widget_type_category,.widget_type_tag {
            padding-bottom:2.5rem;
            border-bottom:1px solid #47b7cc
        }
        .widget_type_category small,.widget_type_tag small {
            margin-left:2px;
            color:#362f3c
        }
        .widget_type_category small:before,.widget_type_tag small:before {
            padding-right:2px;
            content:'('
        }
        .widget_type_category small:after,.widget_type_tag small:after {
            padding-left:2px;
            content:')'
        }
        .footer {
            font-size:1.2rem;
            max-width:640px;
            margin:0 auto auto;
            padding:0 1rem 3rem;
            color:#999
        }
        .footer a {
            color:inherit
        }
        .footer small {
            font-size:inherit;
            display:inline-block;
            margin-left:1rem
        }
        .article__entry .gist {
            line-height:1.6;
            overflow:auto;
            margin-top:15px;
            padding:7px 15px;
            color:#666;
            border:1px solid #7c848c;
            border-radius:2px;
            background:#eee
        }
        .article__entry .gist .gist-file {
            font-family:inherit;
            font-size:.9em;
            margin:0;
            border:none
        }
        .article__entry .gist .gist-file .gist-data {
            border-bottom:none;
            background:none
        }
        .article__entry .gist .gist-file .gist-data pre {
            font-family:font-mono;
            padding:0
        }
        .article__entry .gist .gist-file .gist-meta {
            font-size:100%;
            margin-top:5px;
            padding:0;
            color:#362f3c;
            background:none
        }
        .article__entry figure {
            margin:0
        }
        .article__entry figure.highlight {
            line-height:1.1;
            overflow-x:auto;
            margin:1em 0;
            padding:7px 15px;
            border:1px solid #7c848c;
            border-radius:2px;
            background:#fff
        }
        .article__entry figure.highlight figcaption {
            position:relative;
            margin-bottom:5px;
            color:#362f3c
        }
        .article__entry figure.highlight figcaption a {
            position:absolute;
            right:0
        }
        .article__entry figure pre {
            margin:0;
            padding:0;
            border:none
        }
        .article__entry figure table {
            margin-top:0;
            border-spacing:0
        }
        .article__entry figure table td {
            border:none
        }
        .article__entry figure .gutter {
            padding:0 15px 0 0;
            text-align:right;
            color:#362f3c;
            border-right:1px solid #7c848c
        }
        .article__entry figure .code {
            padding:0 0 0 15px;
            color:#2e2e2e;
            border-left:1px solid #fff
        }
        .article__entry figure .line {
            line-height:20px
        }
        .article__entry pre .bash .shebang,.article__entry pre .comment,.article__entry pre .javascript .javadoc {
            color:#969896
        }
        .article__entry pre .javascript .regexp,.article__entry pre .json .attribute,.article__entry pre .php .regexp,.article__entry pre .scss .built_in,.article__entry pre .string,.article__entry pre .xml .value {
            color:#df5000
        }
        .article__entry pre .css .at_rule,.article__entry pre .css .important,.article__entry pre .javascript .javadoctag,.article__entry pre .javascript .tag,.article__entry pre .keyword,.article__entry pre .php .phpdoc,.article__entry pre .scss .at_rule,.article__entry pre .scss .important,.article__entry pre .scss .tag {
            color:#a71d5d
        }
        .article__entry pre .bash .built_in,.article__entry pre .bash .literal,.article__entry pre .css .attribute,.article__entry pre .css .function,.article__entry pre .css .hexcolor,.article__entry pre .css .number,.article__entry pre .javascript .built_in,.article__entry pre .javascript .literal,.article__entry pre .javascript .number,.article__entry pre .json .number,.article__entry pre .php .literal,.article__entry pre .php .number,.article__entry pre .scss .attribute,.article__entry pre .scss .function,.article__entry pre .scss .hexcolor,.article__entry pre .scss .number,.article__entry pre .scss .preprocessor {
            color:#0086b3
        }
        .article__entry pre .css .tag {
            color:#63a35c
        }
        .article__entry pre .bash .variable,.article__entry pre .css .attr_selector,.article__entry pre .css .value,.article__entry pre .javascript .tag .title,.article__entry pre .scss .variable {
            color:#333
        }
        .article__entry pre .bash .title,.article__entry pre .css .class,.article__entry pre .css .id,.article__entry pre .css .pseudo,.article__entry pre .javascript .title,.article__entry pre .php .title,.article__entry pre .scss .class,.article__entry pre .scss .id,.article__entry pre .scss .pseudo {
            color:#795da3
        }
        .footer {
            text-align:center
        }
        .sidebar-container {
            color: #bfbfbf;
            margin: 25px auto 0 5px;
        }
        .sidebar-container a:hover,
        .sidebar-container a:active {
            color: #47b7cc;
            border-color: #47b7cc;
        }
        .tags a {
            display: inline-block;
            border: 1px solid #bfbfbf;
            border-radius: 999em;
            padding: 0 10px;
            color: #bfbfbf;
            line-height: 24px;
            font-size: 12px;
            text-decoration: none;
            margin: 0 1px;
            margin-bottom: 6px;
        }
    </style>
    <% if (isVimeoContain){ %>
    <script async custom-element="amp-vimeo" src="https://cdn.ampproject.org/v0/amp-vimeo-0.1.js"></script>
    <% } %>
    <% if (isVideoContain){ %>
    <script async custom-element="amp-video" src="https://cdn.ampproject.org/v0/amp-video-0.1.js"></script>
    <% } %>
    <% if (isYoutubeContain){ %>
    <script async custom-element="amp-youtube" src="https://cdn.ampproject.org/v0/amp-youtube-0.1.js"></script>
    <% } %>
    <% if (isTwitterContain){ %>
    <script async custom-element="amp-twitter" src="https://cdn.ampproject.org/v0/amp-twitter-0.1.js"></script>

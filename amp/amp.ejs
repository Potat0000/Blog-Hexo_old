    <script async custom-element="amp-analytics" src="https://cdn.ampproject.org/v0/amp-analytics-0.1.js"></script>
    <script async src="https://cdn.ampproject.org/v0.js"></script>
</head>

<body>
    <header class="header" role="banner">
        <div class="header__logo">
            <h1 class="header__logo__mainHeadline">
                <a ondblclick="window.location.href='<%- config.root %>'" onselectstart="return false">
                    <%= config.title %>
                </a>
            </h1>
            <h2 class="header__logo__subHeadline">
                <a ondblclick="window.location.href='<%- config.root %><%= post.path %>'" onselectstart="return false">
                    <%= config.subtitle %>
                </a>
            </h2>
        </div>
    </header>
    <main role="main">
        <div id="content">
            <article class="article post">
                <header>
                    <h1 class="article__header__title">
                        <%= post.title %>
                    </h1>
                    <div class="article__header__meta">
                        <time class="article__header__meta__time" datetime="<%= post.date.toISOString() %>">
                            <a>
                                Posted on <%= post.date.format("YYYY-MM-DD") %>
                            </a>
                        </time>
                    </div>
                </header>
                <div class="article__entry">
                    <%- post.content %>
                </div>
                <footer class="article__footer clearfix">
                    <div class="sidebar-container">
                        <section>
                            <div class="tags">
                                <% post.tags.forEach(function(tag){ %>
                                    <a href="<%= config.root %>tags/amp/#<%= tag.name %>"><%= tag.name %></a>
                                <% }) %>
                            </div>
                        </section>
                    </div>
                </footer>
                <nav class="pagination">
                    <% if (post.prev){ %>
                        <a href="<%- config.root %><%- post.prev.path %>amp/" title="<%- post.prev.title %>" class="pagination__prev">Prev</a>
                    <% } %>
                    <% if (post.next){ %>
                        <a href="<%- config.root %><%- post.next.path %>amp/" title="<%- post.next.title %>" class="pagination__next">Next</a>
                    <% } %>
                </nav>
            </article>

        </div>
    </main>
    <footer class="footer" role="contentinfo">
        Copyright &copy; <%= new Date().getFullYear() %> - <a href="/" target="_blank" rel="noopener">Gyj1109</a> <br>
        Theme by <a href="https://github.com/hirofumii">hirofumi i</a> • Hosted by <a href="https://pages.github.com/">GitHub Pages</a>
    </footer>
</body>
</html>
---
layout: false
---
<!DOCTYPE html>
<html amp lang="zh-cmn-Hans">
<head>
    <meta charset="utf-8">
    <title>
        <% if (page.title) { %>
            <%= page.title + ' - ' + config.SEOTitle %>
        <% } else{%>
            <%= config.SEOTitle %>
        <% } %>
    </title>
    <!-- Icon -->
    <link rel="apple-touch-icon" sizes="180x180" href="/img/icons/main/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/img/icons/main/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="194x194" href="/img/icons/main/favicon-194x194.png">
    <link rel="icon" type="image/png" sizes="192x192" href="/img/icons/main/android-chrome-192x192.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/img/icons/main/favicon-16x16.png">
    <link rel="manifest" href="/img/icons/main/manifest.json">
    <link rel="mask-icon" href="/img/icons/main/safari-pinned-tab.svg" color="#47b7cc">
    <meta name="apple-mobile-web-app-title" content="Gyj1109's Blog">
    <meta name="application-name" content="Gyj1109's Blog">
    <meta name="msapplication-TileColor" content="#47b7cc">
    <meta name="msapplication-TileImage" content="/img/icons/main/mstile-144x144.png">
    <meta name="msapplication-config" content="/img/icons/main/browserconfig.xml">
    <meta name="theme-color" content="#47b7cc">
    <link rel="canonical" href="<%= config.url + config.root + (page.path||'').replace('index.html', '') %>">
    <meta name="viewport" content="width=device-width,minimum-scale=1,initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="Gyj1109">
    <meta name="google-site-verification" content="1mLbabSiT3xt-4hyZhulqgkKCW_h0ww28mgQNjSp1_I" />
    <style amp-custom="">
        html {
            font-size:62.5%;
            color:#2c2c2c;
            background:#fcfcfc
        }
        html:lang(en) {
            font-family:Helvetica Neue,Helvetica,Arial,sans-serif
        }
        html:lang(zh-cmn-Hans) {
            font-family:Helvetica Neue,Helvetica,Arial,微软雅黑,黑体,Microsoft YaHei,メイリオ,Meiryo,sans-serif
        }
        a {
            text-decoration:none;
            color:#47b7cc
        }
        a:hover {
            text-decoration:underline
        }
        .clearfix:after,.pagination:after {
            display:block;
            clear:both;
            overflow:hidden;
            height:0;
            content:''
        }
        .header {
            max-width:640px;
            margin:auto;
            padding:5rem 1rem 2rem;
            border-top:5px solid #47b7cc
        }
        .header__logo {
            text-align:center
        }
        .header__logo a:hover {
            text-decoration:none
        }
        .header__logo__mainHeadline {
            font-size:2.5rem;
            font-weight:400;
            display:inline-block;
            margin:0 0 3.5rem
        }
        .header__logo__subHeadline {
            font-weight:400;
            margin:15px 0
        }
        .header__menu {
            overflow:hidden
        }
        .header__menu__list {
            overflow:hidden;
            margin:0;
            padding:0;
            list-style:none;
            text-align:center
        }
        .header__menu__list__item {
            display:inline-block;
            margin:0 1rem
        }
        .header__menu__list__item a {
            font-size:1.4rem
        }
        @media screen and (min-width:35.5em) {
            .header {
                display:-webkit-flex;
                display:-ms-flexbox;
                display:flex;
                padding-right:0;
                padding-left:0;
                -webkit-align-items:center;
                -ms-flex-align:center;
                align-items:center
            }
            .header__logo {
                margin: 0 auto;
                text-align:left
            }
            .header__logo__mainHeadline {
                margin:0
            }
            .header__menu {
                float:right
            }
        }
        #content {
            max-width:640px;
            margin:auto;
            padding:0 1rem
        }
        .article {
            font-size:1.6rem;
            margin-bottom:6rem
        }
        .article__header__meta {
            overflow:hidden;
            height:20px;
            text-align:center
        }
        .article__header__meta__time {
            display:block;
            float:left;
            height:100%
        }
        .article__header__meta__time a {
            font-size:1.3rem;
            font-weight:700;
            display:block;
            text-decoration:none;
            color:#aaa
        }
        .article__header__title {
            font-size:2.5rem;
            font-weight:400
        }
        .article__header__title a {
            color:inherit
        }
        .article__header__title a:hover {
            text-decoration:none
        }
        .article.link .article__header__title {
            margin:1rem 0
        }
        .article__entry {
            line-height:2;
            margin-bottom:15px;
            padding-top:1rem
        }
        .article__entry .video-container,.article__entry blockquote,.article__entry dl,.article__entry h3,.article__entry h4,.article__entry h5,.article__entry h6,.article__entry iframe,.article__entry ol,.article__entry p,.article__entry table,.article__entry ul {
            margin-top:15px
        }
        .article__entry blockquote {
            font-family:Georgia,Times New Roman,serif;
            margin:3rem .5rem;
            padding:0 20px;
            border-left:1px solid #47b7cc
        }
        .article__entry blockquote footer {
            font-size:.8em;
            line-height:1;
            margin:20px 0 0;
            padding-top:0;
            color:#4e5359;
            border-top:none
        }
        .article__entry blockquote footer cite:before {
            padding:0 .5em;
            content:'—';
            color:#ccc
        }
        .article__entry code,.article__entry pre {
            font-family:Monaco,Menlo,Consolas,Courier New,monospace
        }
        .article__entry code {
            font-size:.9em;
            margin:0 2px;
            padding:0 5px;
            color:#666;
            border:1px solid #7c848c;
            border-radius:3px;
            background:#f9f9f9
        }
        .article__entry pre {
            overflow:auto;
            padding:7px 15px;
            border:1px solid #7c848c
        }
        .article__entry pre code {
            margin:0;
            padding:0;
            border:none;
            border-radius:0;
            background:none
        }
        .article__entry dl,.article__entry ol,.article__entry ul {
            margin-left:20px;
            padding:0
        }
        .article__entry dl dl,.article__entry dl ol,.article__entry dl ul,.article__entry ol dl,.article__entry ol ol,.article__entry ol ul,.article__entry ul dl,.article__entry ul ol,.article__entry ul ul {
            margin-top:0
        }
        .article__entry h1,.article__entry h2 {
            font-weight:400;
            line-height:1;
            margin-top:1.5em
        }
        .article__entry h3,.article__entry h4,.article__entry h5,.article__entry h6 {
            font-weight:400;
            line-height:1
        }
        .article__entry img,.article__entry video {
            max-width:100%;
            height:auto;
            border:none
        }
        .article__entry iframe {
            border:none
        }
        .article__entry .caption {
            font-size:.9em;
            position:relative;
            display:block;
            margin-top:5px;
            padding-left:25px;
            color:#362f3c
        }
        .article__entry .video-container {
            position:relative;
            overflow:hidden;
            height:0;
            padding-top:30px;
            padding-bottom:56.25%
        }
        .article__entry .video-container embed,.article__entry .video-container iframe,.article__entry .video-container object {
            position:absolute;
            top:0;
            left:0;
            width:100%;
            height:100%;
            margin-top:0
        }
        .article__entry .pullquote {
            font-size:1.5em;
            float:right;
            width:45%;
            margin:1em 0 .5em 1.5em;
            padding:0;
            text-align:left;
            border:none
        }
        .article__entry table {
            border-collapse:collapse
        }
        .article__entry table td {
            padding:2px 10px;
            border:1px solid #7c848c
        }
        .article__footer {
            padding:10px 0;
            border-bottom:1px solid #47b7cc
        }
        .article__footer>div {
            margin:10px 0
        }
        .article__footer a {
            padding-right:1rem
        }
        .pagination {
            font-size:1.4rem;
            margin:3rem 0
        }
        .pagination a {
            font-weight:700;
            display:inline-block;
            padding:1rem 1.1rem;
            border:1px solid #47b7cc;
            border-radius:4px
        }
        .pagination__prev {
            float:left
        }
        .pagination__prev:before {
            display:inline-block;
            margin-right:.5rem;
            content:'«';
            text-align:center
        }
        .pagination__next {
            float:right
        }
        .pagination__next:after {
            display:inline-block;
            margin-left:.5rem;
            content:'»';
            text-align:center
        }
        .archive {
            border-bottom:1px solid #47b7cc
        }
        .archive-title {
            font-size:2.5rem;
            font-weight:400;
            color:#362f3c
        }
        .search input {
            font-size:1.6rem;
            box-sizing:border-box;
            width:100%;
            padding:1rem;
            color:#362f3c;
            border:1px solid #7c848c
        }
        .search input:focus {
            border-color:#47b7cc
        }
        .widget {
            margin-bottom:30px
        }
        .widget ul.entry {
            font-size:14px;
            line-height:2;
            overflow:hidden;
            margin:0;
            padding:0;
            list-style:none
        }
        .widget ul.entry li {
            float:left;
            margin-right:30px
        }
        .widget_type_category,.widget_type_tag {
            padding-bottom:2.5rem;
            border-bottom:1px solid #47b7cc
        }
        .widget_type_category small,.widget_type_tag small {
            margin-left:2px;
            color:#362f3c
        }
        .widget_type_category small:before,.widget_type_tag small:before {
            padding-right:2px;
            content:'('
        }
        .widget_type_category small:after,.widget_type_tag small:after {
            padding-left:2px;
            content:')'
        }
        .footer {
            font-size:1.2rem;
            max-width:640px;
            margin:3rem auto auto;
            padding:3rem 1rem;
            color:#999
        }
        .footer a {
            color:inherit
        }
        .footer small {
            font-size:inherit;
            display:inline-block;
            margin-left:1rem
        }
        .article__entry .gist {
            line-height:1.6;
            overflow:auto;
            margin-top:15px;
            padding:7px 15px;
            color:#666;
            border:1px solid #7c848c;
            border-radius:2px;
            background:#eee
        }
        .article__entry .gist .gist-file {
            font-family:inherit;
            font-size:.9em;
            margin:0;
            border:none
        }
        .article__entry .gist .gist-file .gist-data {
            border-bottom:none;
            background:none
        }
        .article__entry .gist .gist-file .gist-data pre {
            font-family:font-mono;
            padding:0
        }
        .article__entry .gist .gist-file .gist-meta {
            font-size:100%;
            margin-top:5px;
            padding:0;
            color:#362f3c;
            background:none
        }
        .article__entry figure {
            margin:0
        }
        .article__entry figure.highlight {
            line-height:1.1;
            overflow-x:auto;
            margin:1em 0;
            padding:7px 15px;
            border:1px solid #7c848c;
            border-radius:2px;
            background:#fff
        }
        .article__entry figure.highlight figcaption {
            position:relative;
            margin-bottom:5px;
            color:#362f3c
        }
        .article__entry figure.highlight figcaption a {
            position:absolute;
            right:0
        }
        .article__entry figure pre {
            margin:0;
            padding:0;
            border:none
        }
        .article__entry figure table {
            margin-top:0;
            border-spacing:0
        }
        .article__entry figure table td {
            border:none
        }
        .article__entry figure .gutter {
            padding:0 15px 0 0;
            text-align:right;
            color:#362f3c;
            border-right:1px solid #7c848c
        }
        .article__entry figure .code {
            padding:0 0 0 15px;
            color:#2e2e2e;
            border-left:1px solid #fff
        }
        .article__entry figure .line {
            line-height:20px
        }
        .article__entry pre .bash .shebang,.article__entry pre .comment,.article__entry pre .javascript .javadoc {
            color:#969896
        }
        .article__entry pre .javascript .regexp,.article__entry pre .json .attribute,.article__entry pre .php .regexp,.article__entry pre .scss .built_in,.article__entry pre .string,.article__entry pre .xml .value {
            color:#df5000
        }
        .article__entry pre .css .at_rule,.article__entry pre .css .important,.article__entry pre .javascript .javadoctag,.article__entry pre .javascript .tag,.article__entry pre .keyword,.article__entry pre .php .phpdoc,.article__entry pre .scss .at_rule,.article__entry pre .scss .important,.article__entry pre .scss .tag {
            color:#a71d5d
        }
        .article__entry pre .bash .built_in,.article__entry pre .bash .literal,.article__entry pre .css .attribute,.article__entry pre .css .function,.article__entry pre .css .hexcolor,.article__entry pre .css .number,.article__entry pre .javascript .built_in,.article__entry pre .javascript .literal,.article__entry pre .javascript .number,.article__entry pre .json .number,.article__entry pre .php .literal,.article__entry pre .php .number,.article__entry pre .scss .attribute,.article__entry pre .scss .function,.article__entry pre .scss .hexcolor,.article__entry pre .scss .number,.article__entry pre .scss .preprocessor {
            color:#0086b3
        }
        .article__entry pre .css .tag {
            color:#63a35c
        }
        .article__entry pre .bash .variable,.article__entry pre .css .attr_selector,.article__entry pre .css .value,.article__entry pre .javascript .tag .title,.article__entry pre .scss .variable {
            color:#333
        }
        .article__entry pre .bash .title,.article__entry pre .css .class,.article__entry pre .css .id,.article__entry pre .css .pseudo,.article__entry pre .javascript .title,.article__entry pre .php .title,.article__entry pre .scss .class,.article__entry pre .scss .id,.article__entry pre .scss .pseudo {
            color:#795da3
        }
        .footer {
            text-align:center
        }
        .sidebar-container .tags a {
            border-color:#bfbfbf
        }
        .sidebar-container .tags a:hover,.sidebar-container .tags a:active {
            border-color:#47b7cc
        }
        .tags {
            margin-bottom:-5px
        }
        .tags a,.tags .tag {
            display:inline-block;
            border:1px solid rgba(255,255,255,0.8);
            border-radius:999em;
            padding:0 10px;
            line-height:24px;
            font-size:12px;
            text-decoration:none;
            margin:0 1px;
            margin-bottom:6px
        }
        .tags a:hover,.tags .tag:hover,.tags a:active,.tags .tag:active {
            color:white;
            border-color:white;
            text-decoration:none
        }
        @media only screen and (min-width:768px) {
            .tags a,.tags .tag {
                margin-right:5px
            }
        }
        #tag-heading {
            padding:70px 0 60px
        }
        @media only screen and (min-width:768px) {
            #tag-heading {
                padding:55px 0
            }
        }
        #tag_cloud {
            margin:20px 0 15px 0
        }
        #tag_cloud a,#tag_cloud .tag {
            font-size:14px;
            border:0;
            line-height:28px;
            margin:0 2px;
            margin-bottom:8px;
            background:#d6d6d6
        }
        .one-tag-list .tag-text {
            font-weight:200;
            font-family:-apple-system,BlinkMacSystemFont,"Helvetica Neue","Arial","PingFang SC","Hiragino Sans GB","STHeiti","Microsoft YaHei","Microsoft JhengHei","Source Han Sans SC","Noto Sans CJK SC","Source Han Sans CN","Noto Sans SC","Source Han Sans TC","Noto Sans CJK TC","WenQuanYi Micro Hei",SimSun,sans-serif;
            line-height:1.7
        }
        .one-tag-list .post-preview {
            position:relative
        }
        .one-tag-list .post-preview>a .post-title {
            font-size:16px;
            font-weight:500;
            margin-top:20px
        }
        .one-tag-list .post-preview>a .post-subtitle {
            font-size:12px
        }
        .one-tag-list .post-preview>.post-meta {
            position:absolute;
            right:5px;
            bottom:0;
            margin:0;
            font-size:12px;
            line-height:12px
        }
        @media only screen and (min-width:768px) {
            .one-tag-list .post-preview {
                margin-left:20px
            }
            .one-tag-list .post-preview>a>.post-title {
                font-size:18px;
                line-height:1.3
            }
            .one-tag-list .post-preview>a>.post-subtitle {
                font-size:14px
            }
            .one-tag-list .post-preview .post-meta {
                font-size:18px
            }
        }
        a {
            background-color:transparent
        }
        bootstrap.css:6 * {
            -webkit-box-sizing:border-box;
            -moz-box-sizing:border-box;
            box-sizing:border-box
        }
        #tag_cloud a:hover,#tag_cloud .tag:hover,#tag_cloud a:active,#tag_cloud .tag:active {
            background-color:#47b7cc
        }
        @media only screen and (min-width:768px) {
            #tag_cloud {
                margin-bottom:25px
            }
        }
        .tag-comments {
            font-size:12px
        }
        @media only screen and (min-width:768px) {
            .tag-comments {
                font-size:14px
            }
        }
        .t:first-child {
            margin-top:0
        }
        .listing-seperator {
            color:#47b7cc;
            font-size:21px;
            padding-top:25px;
            margin-top:-25px
        }
        .listing-seperator::before {
            margin-right:5px
        }
        @media only screen and (min-width:768px) {
            .listing-seperator {
                font-size:20px;
                line-height:2
            }
        }
        .sidebar-container a:active,.sidebar-container a:hover {
            color:#47b7cc;
        }
        .sidebar-container a {
            color:#bfbfbf;
        }
        .one-tag-list {
            position: relative;
            top: 35px;
            margin-top: 25px;
        }
        #main {
            position: relative;
            margin-top: -40px;
        }
    </style>
    <% if (config.generator_amp.google_analytics){ %>
    <script async custom-element="amp-analytics" src="https://cdn.ampproject.org/v0/amp-analytics-0.1.js"></script>
    <% } %>
    <script type="application/ld+json">
		{
			"@context": "https://schema.org",
			"@type": "BlogPosting",
			"headline": "<%= page.title %>",
			<% if (page.description){ %>
			"description": "<%= page.description %>",
			<% } else if (page.excerpt){ %>
			"description": "<%= page.excerpt %>",
			<% } %>
			"mainEntityOfPage":{
			    "@type":"WebPage",
			    "@id":"<%= config.url %>/<%= page.path %>"
			},
			"datePublished": "<%= page.date.toISOString() %>",
			"dateModified": "<%= page.updated.toISOString() %>",
			"author": {
			    "@type": "Person",
			    "name": "<%= config.author %>"
			},
			"publisher": {
			    "@type": "Organization",
			    "name": "<%= config.title %>",
                "logo": "/img/avatar-gyj1109.jpg"
			},
            "cancelimage": "/img/icons/main/android-chrome-512x512.png"
		}
	</script>
    <style amp-boilerplate>body{-webkit-animation:-amp-start 8s steps(1,end) 0s 1 normal both;-moz-animation:-amp-start 8s steps(1,end) 0s 1 normal both;-ms-animation:-amp-start 8s steps(1,end) 0s 1 normal both;animation:-amp-start 8s steps(1,end) 0s 1 normal both}@-webkit-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@-moz-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@-ms-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@-o-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}</style><noscript><style amp-boilerplate>body{-webkit-animation:none;-moz-animation:none;-ms-animation:none;animation:none}</style></noscript>
    <script async custom-element="amp-analytics" src="https://cdn.ampproject.org/v0/amp-analytics-0.1.js"></script>
    <script async src="https://cdn.ampproject.org/v0.js"></script>
</head>

<body>
    <header class="header" role="banner">
        <div class="header__logo">
            <h1 class="header__logo__mainHeadline">
                <a ondblclick="window.location.href='<%- config.root %>'" onselectstart="return false">
                    <%= config.title %>
                </a>
            </h1>
            <h2 class="header__logo__subHeadline">
                <a ondblclick="window.location.href='<%- config.root %>tags/'" onselectstart="return false">
                    <%= config.subtitle %>
                </a>
            </h2>
        </div>
    </header>
    <main role="main">
        <div id="content">

            <!-- Main Content -->
            <div class="container">
                <div class="row">
                    <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
                        <!-- 标签云 -->
                        <div class="sidebar-container">
                            <section>
                                <div class="tags">
                                    <% site.tags.map(function(tag){  %>
                                        <a href="#<%= tag.name %>" title="<%= tag.name %>"><%= tag.name %></a>
                                    <% }) %>
                                </div>
                            </section>
                        </div>

                        <!-- 标签列表 -->
                        <div id="main">
                            <% site.tags.map(function(tag){  %>
                            <div class="one-tag-list">
                                <span class="fa fa-tag listing-seperator" id="<%= tag.name %>">
                                    <span class="tag-text" ondblclick="window.location.href='<%- config.root %>tags/#<%= tag.name %>'" onselectstart="return false"><%= tag.name %></span>
                                </span>
                                <% tag.posts.map(function(post){  %>
                                <!-- <li class="listing-item">
                                <time datetime="{{ post.date | date:"%Y-%m-%d" }}">{{ post.date | date:"%Y-%m-%d" }}</time>
                                <a href="{{ post.url }}" title="{{ post.title }}">{{ post.title }}</a>
                                </li> -->
                                <div class="post-preview">
                                    <a href="<%- config.root %><%- post.path %>amp/">
                                        <h2 class="post-title">
                                            <%- post.title || "Untitled" %>
                                        </h2>
                                        <% if (post.subtitle && post.subtitle.length) { %>
                                            <h3 class="post-subtitle">
                                                <%- post.subtitle %>
                                            </h3>
                                        <% } %>
                                    </a>
                                    <!-- <p class="post-meta">{{ post.date | date:"%Y-%m-%d" }}</p> -->
                                </div>

                                <% }) %>
                            </div>
                            <% }) %>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </main>
    <footer class="footer" role="contentinfo">
        Copyright &copy; <%= new Date().getFullYear() %> - <a href="/" target="_blank" rel="noopener">Gyj1109</a> <br>
        Theme by <a href="https://github.com/hirofumii">hirofumi i</a> • Hosted by <a href="https://pages.github.com/">Github Pages</a>
    </footer>
</body>
</html>